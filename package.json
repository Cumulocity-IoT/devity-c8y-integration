{
  "name": "devity-sps-demo",
  "version": "0.3.1",
  "description": "",
  "scripts": {
    "start": "ng serve -u https://partner-devity.eu-latest.cumulocity.com",
    "start:admin": "npm start -- administration --shell administration",
    "start:dm": "npm start -- devicemanagement --shell devicemanagement --port 4201",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:admin && npm run build:dm",
    "prebuild:admin": "rimraf dist/administration",
    "build:admin": "ng build administration",
    "postbuild:admin": "node -e \"require('fs').rename('dist/administration.zip', 'dist/${npm_package_name}_administration_${npm_package_version}.zip', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "prebuild:dm": "rimraf dist/devicemanagement",
    "build:dm": "ng build devicemanagement",
    "postbuild:dm": "node -e \"require('fs').rename('dist/devicemanagement.zip', 'dist/${npm_package_name}_devicemanagement_${npm_package_version}.zip', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "locale-extract": "c8ycli locale-extract"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@c8y/bootstrap": "1022.33.0",
    "@c8y/client": "1022.33.0",
    "@c8y/ngx-components": "1022.33.0",
    "@c8y/style": "1022.33.0",
    "file-saver": "^2.0.5",
    "monaco-editor": "~0.53.0",
    "ngx-bootstrap": "19.0.2",
    "ngx-moment": "^6.0.2",
    "rxjs": "7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.7",
    "@angular/cli": "^19.0.7",
    "@angular/compiler-cli": "^19.0.0",
    "@c8y/devkit": "1022.33.0",
    "@c8y/options": "1022.33.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.6.2",
    "@types/lodash": "4.17.20",
    "lodash": "4.17.21",
    "rimraf": "5.0.5"
  }
}
