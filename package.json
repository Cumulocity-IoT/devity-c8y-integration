{
  "name": "devity-sps-demo",
  "version": "0.2.0",
  "description": "",
  "scripts": {
    "start": "ng serve -u https://partner-devity.eu-latest.cumulocity.com",
    "start:admin": "npm start -- administration --shell administration",
    "start:dm": "npm start -- devicemanagement --shell devicemanagement --port 4201",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:admin && npm run build:dm",
    "prebuild:admin": "rimraf dist/administration",
    "build:admin": "ng build administration",
    "postbuild:admin": "node -e \"require('fs').rename('dist/administration.zip', 'dist/${npm_package_name}_administration_${npm_package_version}.zip', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "prebuild:dm": "rimraf dist/devicemanagement",
    "build:dm": "ng build devicemanagement",
    "postbuild:dm": "node -e \"require('fs').rename('dist/devicemanagement.zip', 'dist/${npm_package_name}_devicemanagement_${npm_package_version}.zip', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "locale-extract": "c8ycli locale-extract"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@c8y/bootstrap": "1020.28.1",
    "@c8y/client": "1020.28.1",
    "@c8y/ngx-components": "1020.28.1",
    "@c8y/style": "1020.28.1",
    "@ng-select/ng-select": "^12.0.7",
    "file-saver": "^2.0.5",
    "ngx-moment": "^6.0.2",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.9",
    "@angular/cli": "^17.3.9",
    "@angular/compiler-cli": "^17.3.0",
    "@c8y/devkit": "1020.28.1",
    "@c8y/options": "1020.28.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.5",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lodash": "^4.4.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.4.2"
  }
}
